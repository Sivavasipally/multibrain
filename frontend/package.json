{
  "name": "multibrain-frontend",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "RAG Chatbot PWA Frontend - A comprehensive React application with offline capabilities",
  "author": "RAG Chatbot Development Team",
  "license": "MIT",
  "keywords": ["react", "pwa", "chatbot", "rag", "ai", "offline-first"],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production tsc -b && vite build --mode production",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "preview": "vite preview --host",
    "install:safe": "npm install --legacy-peer-deps",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:debug": "jest --verbose --no-cache",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "serve": "serve -s dist -l 3000"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/utils": "^7.2.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.7.0",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^4.1.2",
    "vite-plugin-pwa": "^1.0.1",
    "workbox-webpack-plugin": "^7.3.0",
    "workbox-window": "^7.3.0",
    "localforage": "^1.10.0",
    "idb": "^8.0.0",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "uuid": "^10.0.0",
    "dompurify": "^3.1.7",
    "marked": "^12.0.2",
    "highlight.js": "^11.11.1",
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/rollup-win32-x64-msvc": "^4.46.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/uuid": "^10.0.0",
    "@types/dompurify": "^3.0.5",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-import": "^2.31.0",
    "fake-indexeddb": "^5.0.1",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "msw": "^2.0.8",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-bundle-analyzer": "^0.12.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "serve": "^14.2.4",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "overrides": {
    "@testing-library/react": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
